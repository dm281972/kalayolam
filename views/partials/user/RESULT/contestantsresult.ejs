<style>
  * {
    margin: 0;
    padding: 0;
    
  }
  .center {
    display: flex;
   
    height: 90%;
    width: 100%;
    align-items: center;
    gap: 30px;
  }
  .body {
    height: 92vh;
    width: 100%;
   }
  .sidebar{
    left: 0;
    width: 250px;
    background-color: aquamarine;

  }
  .header{
    top: 0;
    width: 100%;
    height: 50px ;
    background: rgb(34, 34, 105)
  }
  .w-100{
    width: 100%;
  }
  .h-100{
    height: 100%;
  }
  .c {
    display: flex;
  }
</style>
<div class="body" >
  <div class="header"> 

  </div>
  <div class="w-100 h-100 c">
    <div class="sidebar">
      <h3 style="text-align: center;">sidebar</h3>
      <div style="height: 100px; width: 200px;border: 2px solid brown;margin-left:20px ;">
        <form action="">
          <label >section</label>
          <%- include('../../../partials/sectionForm.ejs') %>
          <button type="submit">select</button>
        </form>
        </div>
    </div>
    <div style="overflow: auto; flex: 1 1 0%;">
      
      
        <% contestants.forEach((contestant) => { %>
          <a href="/results/contestants/<%=contestant._id %>"> 
              <div style="display: flex; justify-content: center; padding: 10px; ">
                <p >Name: <%= contestant.name %></p>
                <p >Total Points: <%= contestant.totalPoints %></p>
              </div>
          </a>
            <% }); %>
      
    </div>
  </div>
  
   
  </div>


